{
  "name": "gen-ldm",
  "version": "1.0.0",
  "author": "me",
  "license": "ISC",
  "description": "",
  "main": "public/electron.js",
  "homepage": ".",
  "scripts": {
    "electron:start": "npm run transpile && npm run electron:dev",
    "ts:build": "tsc electron/main.ts  --esModuleInterop true; mv electron/main.js public/electron.js",
    "transpile": "npm run react:build && npm run ts:build",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run react:start\" \"wait-on http://localhost:3000 && tsc -p electron & electron .\"",
    "dist": "npm run transpile && electron-builder -c.extraMetadata.main=build/electron.js",
    "start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject"
  },
  "build": {
    "extends": null,
    "appId": "com.example.electron-cra",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "@types/html2json": "^1.0.0",
    "@types/http-errors": "^1.8.2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.9",
    "electron-builder": "^22.14.5",
    "react-icons": "^4.4.0",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.7",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@tinymce/tinymce-react": "^4.1.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.13",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "bcryptjs": "^2.4.3",
    "chakra-ui-autocomplete": "^1.4.5",
    "electron-is-dev": "^2.0.0",
    "framer-motion": "^6.2.8",
    "html2json": "^1.0.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
